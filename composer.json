{
  "name": "vimkanzogh/laravel-hellio",
  "description": "Laravel integration for the Hellio Messaging array of services.",
  "type": "library",
  "keywords": ["laravel", "hellio messaging", "integration", "sms", "otp", "notification", "channel", "voice sms"],
  "license": "MIT",
  "authors": [
    {
      "name": "Albert ninyeh",
      "email": "eaglesecurity0@gmail.com",
      "homepage": "https://github.com/VimKanzoGH"
    }
  ],
  "autoload": {
    "psr-4": {
      "HellioSolutions\\HellioMessaging\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "HellioSolutions\\HellioMessaging\\ServiceProvider"
      ],
      "aliases": {
        "HellioMessaging": "HellioSolutions\\HellioMessaging\\Facade"
      }
    }
  },
  "prefer-stable": true,
  "require": {
    "php": ">=7.2",
    "guzzlehttp/guzzle": "^7.2",
    "illuminate/notifications": "5.*|^6.0|^7.0|^8.0|^9.0",
    "illuminate/support": "5.*|^6.0|^7.0|^8.0|^9.0",
    "ext-json": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^6.4|^7.0",
    "phpunit/phpunit": "^9.5.1"
  }
}
